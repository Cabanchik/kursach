<Application x:Class="kursach.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:kursach"
             StartupUri="Windows/LogWin.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="nigger" UriSource="../resoursess/anon.jpg"></BitmapImage>
            <FontFamily x:Key="yakuza" >resoursess/edosz.ttf</FontFamily>
            <Style TargetType="Button">
                <Setter Property="Background" Value="#F3C892"></Setter>
            </Style>
            <Style TargetType="TextBox">
                <Setter Property="Background" Value="#F3C892"></Setter>
            </Style>
        </ResourceDictionary>
        
       
    </Application.Resources>
    
</Application>
